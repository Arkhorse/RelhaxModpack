<Window x:Class="RelhaxModpack.Windows.ProgressIndicator"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RelhaxModpack.Windows"
        mc:Ignorable="d"
        Title="Loading" Height="200" Width="350" Icon="/RelhaxModpack;component/Resources/modpack_icon.ico" ResizeMode="NoResize">
    <Grid>
        <TextBlock HorizontalAlignment="Stretch" Margin="10,10,10,0" TextWrapping="Wrap" Text="Loading, Please Wait" VerticalAlignment="Top" TextAlignment="Center" x:Name="LoadingHeader"/>
        <TextBox HorizontalAlignment="Stretch" Margin="10,31,10,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Height="101" x:Name="LoadingText"/>
        <ProgressBar HorizontalAlignment="Stretch" Height="20" Margin="10,137,10,0" VerticalAlignment="Top" x:Name="LoadingProgressBar"/>
    </Grid>
</Window>
