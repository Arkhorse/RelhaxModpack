<local:RelhaxWindow x:Class="RelhaxModpack.Windows.NewsViewer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RelhaxModpack.Windows"
        mc:Ignorable="d"
        Title="NewsViewer" Height="450" Width="800">
    <Grid>
        <ListBox HorizontalAlignment="Left" Height="383" Margin="10,26,0,0" VerticalAlignment="Top" Width="382" Background="White"/>
        <TabControl Height="26" VerticalAlignment="Top" Margin="10,0,19,0">
            <TabItem x:Name="ApplicationTab" Header="Application" Margin="-2,-2,-2,2">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="Database" Margin="0,-2,-4,-1" Height="23" VerticalAlignment="Bottom">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>

    </Grid>
</local:RelhaxWindow>
